@{
    var prefixo = "Produto";
    var label = "Produto";
    var id = "0";
    var idLabel = prefixo + "AutoCompleteLabel";
    var idSelect = prefixo + "AutoComplete";
    var idErro = prefixo + "AutoCompleteErro";
}
@*<div class="form-group col-sm-12">*@
<label id='@idLabel'>@label</label>
<select class='select2 form-control' id='@idSelect'></select>
<div class="text-danger" style="font-size: 80%; margin-top: .25rem;" id="@idErro"></div>
<script type="text/javascript" src="~/js/Views/Partial/ProdutoAutocomplete.js"></script>
<script>
    $(function () {
        ProdutoAutoCompleteInicializar("@idSelect",  "@idErro", "@id");
    });
</script>
@*</div>*@
